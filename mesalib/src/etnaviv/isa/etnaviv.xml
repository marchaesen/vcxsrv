	</display>

	<!-- SRC0 -->
	<pattern pos="43">1</pattern> <!-- SRC0_USE -->
	<field name="SRC0_REG" low="44" high="52" type="uint"/>
	<field name="SRC0" low="54" high="63" type="#instruction-src">
		<param name="SRC0_REG" as="SRC_REG"/>
