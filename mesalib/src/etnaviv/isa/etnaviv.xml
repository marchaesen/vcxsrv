<isa>

<template name="INSTR_ALU">
	{NAME}{SAT}{COND}{SKPHP}{TYPE}{PMODE}{THREAD}{RMODE}
</template>

<template name="INSTR_TEX">
	{NAME}
</template>

<template name="INSTR_CF">
	{NAME}{COND}{TYPE}
</template>

<template name="INSTR_LOAD_STORE">
	{NAME}{COND}{SKPHP}{DENORM}{LOCAL}{TYPE}{PMODE}
</template>

